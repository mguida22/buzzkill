{
  "manifest_version": 2,

  "name": "Buzzkill",
  "description": "Down with Buzzfeed! Remove all clickbait style articles",
  "version": "0.1.0",

  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },

  "content_scripts": [{
    "matches": ["*://*/*"],
    "js": ["jquery-2.1.4.min.js", "main.js", "dict.js"]
  }],

  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Buzzkill"
  },

  "permissions": [
    "activeTab",
    "storage"
  ]
}
